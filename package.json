{
  "name": "workshops-ipa",
  "version": "1.0.0",
  "description": "react webapp to schedule and participate in workshops",
  "author": "Oliver Dietsche",
  "scripts": {
    "dev": "next dev",
    "start": "next start",
    "build": "next build",
    "storybook": "start-storybook -p 6006 --no-dll",
    "build-storybook": "build-storybook --no-dll",
    "lint": "npm run lint:js",
    "lint:js": "eslint .",
    "prettier": "prettier --write \"**/*.*(js|jsx|ts|tsx|json|md)\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,md}": [
      "prettier --write"
    ]
  },
  "commitlint": {
    "extends": [
      "@namics/commitlint-conventional-changelog"
    ]
  },
  "dependencies": {
    "@emotion/react": "11.4.0",
    "next": "10.2.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "xss": "1.0.9"
  },
  "devDependencies": {
    "@babel/core": "7.14.3",
    "@babel/preset-react": "7.13.13",
    "@commitlint/cli": "12.1.4",
    "@emotion/babel-plugin": "11.3.0",
    "@emotion/babel-preset-css-prop": "11.2.0",
    "@namics/commitlint-conventional-changelog": "1.2.2",
    "@namics/cz-conventional-changelog": "2.0.3",
    "@namics/eslint-config": "9.0.6",
    "@namics/prettier-config": "2.0.1",
    "@namics/ts-config": "1.1.1",
    "@storybook/addon-actions": "6.2.9",
    "@storybook/addon-essentials": "6.2.9",
    "@storybook/addon-links": "6.2.9",
    "@storybook/react": "6.2.9",
    "@types/node": "15.6.1",
    "@types/react": "17.0.8",
    "@typescript-eslint/parser": "4.25.0",
    "autofiles": "1.0.1",
    "babel-loader": "8.2.2",
    "eslint": "7.27.0",
    "eslint-plugin-import": "2.23.3",
    "husky": "6.0.0",
    "lint-staged": "11.0.0",
    "prettier": "2.3.0",
    "tslib": "2.2.0",
    "typescript": "4.3.2"
  }
}
